#include <stdio.h>

int main()
{
    int arr[5] = {1, 5, 3, 4, 9};
    printf("original array is: ");
    for (int i=0; i<5; i++) { 
        printf("%d ", arr[i]);
    }
    printf("\n");
    printf("reversed array is: ");
    // Loop from the last element to the first to print in reverse order
    for (int i = 4; i >= 0; i--) {
        printf("%d ", arr[i]);
    }
    printf("\n");
    

    return 0;
}
