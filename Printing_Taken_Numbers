
// // Header Files
#include <stdio.h>
#include <conio.h>

#define ARRAY_SIZE 5 // MACROS (all in upper-case letters) 

// // Main Function Start
int main()
{
    int num[ARRAY_SIZE];

    printf("\nEnter 5 Numbers => ");
    for (int i = 0 ; i < ARRAY_SIZE ; i++) { 
        scanf("%d", &num[i]); 
        //printf("%d ", num[i]);
        
}
printf("\n[");
for (int i = 0; i < ARRAY_SIZE ; i++) { 
    printf("%d ", num[i]);
}
printf("]"); 

int max = 0;
for ( int i = 0; i < ARRAY_SIZE ; i++) {
    if(num[i] > max) { 
        max = num[i];
    }
}
printf("\nMax is : %d", max); 
//printf("\nLsit of Numbers = [%d ", num);
   // printf("\nLargest Number => %d", largest);

   
    getch();
    return 0;
}
// // Main Function End

