// // Header Files
#include <stdio.h>
#include <conio.h>

// // Main Function Start
int main()
{
    for(int i = 0; i < 10 ; i++) { 
        printf("%d \t", 2*i+1);
    } 
    printf("\n");
    for(int i = 0; i < 10 ; i++) { 
        printf("%d \t", i*i); 
    } 
    

    getch();
    return 0;
}
// // Main Function End
