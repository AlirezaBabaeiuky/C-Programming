#include <stdio.h>
#include <math.h>
#include <conio.h>

double myf1( n1, n2) { 
    return (double)(n1+n2)/2;
}

int main()
{
    printf("Hello World");
    int n1, n2;
    double result;
    printf("\nEnter 2 numbers here: ");
    scanf("%d %d", &n1, &n2);
    result = myf1(n1, n2);
    printf("\nresult (average value) is: %f", result);

    return 0;
}
