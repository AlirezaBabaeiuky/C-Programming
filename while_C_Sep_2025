#include <stdio.h>
int main() {
    
  // Initialization expression
  int i = 0; 
 
  // Test expression
  while(i <= 5) {
    printf("%d ", i + 1);   
    
    // update expression
    i++;
  } 
  
  return 0;
}

/*
note that while is the same concept as for but in a bit and slightly different syntax 
*/
