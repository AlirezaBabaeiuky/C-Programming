#include <stdio.h>

int main()
{
    int arr[6] = {10, 20, 30, 40, 50};
    int size = sizeof(arr)/sizeof(arr[0]); 
    size = 5; 
    int ele_to_insert = 35; // element to be inserted 
    int idx_to_insert = 3; // index at which the 
    if (idx_to_insert>=0 && idx_to_insert<=size && size<6) { 
        for (int i=size; i>idx_to_insert; i--) { 
            arr[i] = arr[i-1]; 
        }
        arr[idx_to_insert] = ele_to_insert; 
        size++;
    }
    for (int i=0; i<size; i++) { 
        printf("%d ", arr[i]);
    }

    return 0;
}
